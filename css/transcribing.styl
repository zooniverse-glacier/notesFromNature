div.transcribing div#transcriber ul.steps li.completed span.circle {
  border-color: #669933;
  background: #669933;
}
div.transcribing div#transcriber ul.steps li.selected a {
  
  opacity: 1;
  color: #666666;
}
div.transcribing div#transcriber ul.steps li.selected a:hover {
  cursor: default;
}
div.transcribing div#transcriber ul.steps li.selected span.circle {
  border-color: #666666;
  background: none;
