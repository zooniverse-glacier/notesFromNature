  cursor: default;
}
div.transcribing div#transcriber ul.steps li.selected.completed span.circle {
  background: #666666;
  border-color: #666666;
}
div.transcribing div#transcriber ul.steps span.tail {
  position: absolute;
  bottom: -10px;
  right: 11px;
  height: 0;
  width: 0;
  border-top: 6px solid white;
  border-left: 6px solid transparent;
  border-bottom: 6px solid transparent;
  border-right: 6px solid transparent;
}
div.transcribing div.loader {
  position: absolute;
  top: 150px;
  left: 50%;
  width: 60px;
  height: 60px;
  margin: 0 0 0 -25px;
  background: rgba(0, 0, 0, 0.5);
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  -o-border-radius: 5px;
  -ms-border-radius: 5px;
  -khtml-border-radius: 5px;
  border-radius: 5px;
}
div.transcribing div.loader > div {
  margin: 30px;
}
div.transcribing div#transcriber.free .ui-resizable-handle {
  display: block;
}
div.transcribing div#transcriber.free div.bottom:hover, div.transcribing div#transcriber.free div.top:hover {
  cursor: move;
}
div.transcribing div#transcriber.free div.bottom:hover ul, div.transcribing div#transcriber.free div.top:hover ul {
  cursor: default;
}
div.transcribing div#transcriber.free div.bottom:hover div, div.transcribing div#transcriber.free div.top:hover div {
  cursor: default;
}

div.tooltip {
