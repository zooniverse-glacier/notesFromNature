body.transcribingScreen  {
  background: url("../images/bg.jpg") repeat-x #333333;
  overflow: hidden;
}
body.transcribingScreen  header {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  min-width: 1024px;
  height: 80px;
  z-index: 10;
  -moz-box-shadow: rgba(0, 0, 0, 0.6) 0 0 20px 5px;
  -webkit-box-shadow: rgba(0, 0, 0, 0.6) 0 0 20px 5px;
  -o-box-shadow: rgba(0, 0, 0, 0.6) 0 0 20px 5px;
  box-shadow: rgba(0, 0, 0, 0.6) 0 0 20px 5px;
  background: url("../images/bg.jpg") repeat-x #333333;
  border-bottom: 1px solid rgba(255, 255, 255, 0.02);
}
body.transcribingScreen  header div.wrapper {
  width: 100%;
  height: 80px;
  background-image: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, rgba(0, 0, 0, 0)), color-stop(50%, rgba(0, 0, 0, 0)), color-stop(70%, rgba(0, 0, 0, 0.3)), color-stop(100%, rgba(0, 0, 0, 0.5)));
  background-image: -webkit-linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
  background-image: -moz-linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
  background-image: -o-linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
  background-image: -ms-linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
  background-image: linear-gradient(rgba(0, 0, 0, 0), rgba(0, 0, 0, 0) 50%, rgba(0, 0, 0, 0.3) 70%, rgba(0, 0, 0, 0.5));
}
body.transcribingScreen  header div.left {
  float: left;
  height: 60px;
  margin: 0;
  padding: 20px 0 0 24px;
}
body.transcribingScreen  header div.right {
  position: relative;
  float: right;
  height: 55px;
  padding: 25px 24px 0 24px;
}
body.transcribingScreen  header div.right::before {
  position: absolute;
  top: 0;
  left: 1px;
  height: 80px;
  width: 1px;
  content: '';
  border-left: 1px solid rgba(255, 255, 255, 0.1);
}
body.transcribingScreen  header div.right::after {
  position: absolute;
  top: 0;
  left: 0;
  height: 80px;
  width: 1px;
  content: '';
  border-left: 1px solid black;
}
body.transcribingScreen  header h1 {
  font: bold 17px "Open Sans";
  color: white;
  font-weight: 800;
}
body.transcribingScreen  header h2 {
  font: normal 13px "Open Sans";
  color: #999999;
}
body.transcribingScreen  header h5 {
  display: inline-block;
  vertical-align: top;
  font: bold 37px "Open Sans";
  color: white;
  font-weight: 800;
  line-height: 28px;
}
body.transcribingScreen  header label {
  display: inline-block;
  vertical-align: top;
  width: 74px;
  padding: 2px 0 0 5px;
  font: bold 13px "Open Sans";
  color: white;
  line-height: 14px;
}
body.transcribingScreen  header span {
  display: inline-block;
  width: auto;
}
body.transcribingScreen  footer {
  display: none;
  width: 600px;
  margin: 150px auto 150px;
  text-align: center;
}
body.transcribingScreen  footer h3 {
  display: block;
  font: bold 23px "Open Sans";
  font-weight: 800;
  color: #212121;
  text-shadow: 0 1px #414141;
}
body.transcribingScreen  footer p {
  margin: 10px 0 0 0;
  font: normal 15px "Helvetica Neue","Helvetica",Arial;
  color: white;
}
body.transcribingScreen  footer p a {
  color: #0099CC;
}
body.transcribingScreen  footer p a:hover {
  color: #0066CC;
}

div.transcribing {
  position: relative;
  display: block;
  width: auto;
  padding: 0;
  margin: 90px auto;
}
div.transcribing div#transcriber {
  position: absolute;
  display: none;
  
  opacity: 0;
  top: 0;
  left: 0;
  display: block;
  height: 205px;
  margin: px 0 0 0;
  -moz-border-radius: 8px;
  -webkit-border-radius: 8px;
  -o-border-radius: 8px;
  -ms-border-radius: 8px;
  -khtml-border-radius: 8px;
  border-radius: 8px;
  border: 1px solid black;
  -moz-box-shadow: rgba(0, 0, 0, 0.4) 0 0 5px 3px;
  -webkit-box-shadow: rgba(0, 0, 0, 0.4) 0 0 5px 3px;
  -o-box-shadow: rgba(0, 0, 0, 0.4) 0 0 5px 3px;
  box-shadow: rgba(0, 0, 0, 0.4) 0 0 5px 3px;
}
div.transcribing div#transcriber div.top {
  position: absolute;
  top: 4px;
  left: 3px;
  right: 3px;
  bottom: 57px;
  display: block;
  -moz-box-shadow: #444444 0 -1px 0 3px;
  -webkit-box-shadow: #444444 0 -1px 0 3px;
  -o-box-shadow: #444444 0 -1px 0 3px;
  box-shadow: #444444 0 -1px 0 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  -o-border-radius: 3px;
  -ms-border-radius: 3px;
  -khtml-border-radius: 3px;
  border-radius: 3px;
  border: 1px solid black;
  z-index: 0;
}
div.transcribing div#transcriber div.top span.tail {
  position: absolute;
  bottom: -1px;
  height: 5px;
  width: 3px;
  background: #444444;
}
div.transcribing div#transcriber div.top span.tail.left {
  left: -4px;
}
div.transcribing div#transcriber div.top span.tail.right {
  right: -4px;
}
div.transcribing div#transcriber div.bottom {
  position: absolute;
  padding: 12px 13px 12px;
  bottom: 0;
  left: 0;
  right: 0;
  height: 33px;
  display: block;
  background: #444444;
  -moz-border-radius-bottomleft: 6px;
  -webkit-border-bottom-left-radius: 6px;
  -o-border-bottom-left-radius: 6px;
  -ms-border-bottom-left-radius: 6px;
  -khtml-border-bottom-left-radius: 6px;
  border-bottom-left-radius: 6px;
  -moz-border-radius-bottomright: 6px;
  -webkit-border-bottom-right-radius: 6px;
  -o-border-bottom-right-radius: 6px;
  -ms-border-bottom-right-radius: 6px;
  -khtml-border-bottom-right-radius: 6px;
  border-bottom-right-radius: 6px;
}
div.transcribing div#transcriber div.bottom div.right {
  position: relative;
  float: right;
  width: 210px;
}
div.transcribing div#transcriber div.bottom div.right a.checkRecord {
  float: right;
}
div.transcribing div#transcriber div.bottom div.right a.choose_step {
  float: right;
  display: inline-block;
  vertical-align: top;
  background: black;
  height: 13px;
  margin: 0 5px 0 0;
  padding: 10px;
  font: bold 11px "Helvetica",Arial;
  line-height: 13px;
  color: white;
  text-decoration: none;
  text-transform: uppercase;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  -o-border-radius: 3px;
  -ms-border-radius: 3px;
  -khtml-border-radius: 3px;
  border-radius: 3px;
}
div.transcribing div#transcriber div.bottom div.right a.choose_step:hover {
  text-decoration: underline;
}
div.transcribing div#transcriber div.bottom div.right a.choose_step:active {
  padding: 11px 10px 9px 10px;
}
div.transcribing div#transcriber div.bottom div.right div.step_viewer {
  display: none;
}
div.transcribing div#transcriber div.bottom div.right div.step_viewer ul {
  display: none;
}
div.transcribing div#transcriber .ui-resizable-handle {
  position: absolute;
  bottom: 2px;
  right: 2px;
  width: 14px;
  height: 14px;
  background: url("../images/resize.png");
  cursor: s-resize;
  display: none;
}
div.transcribing div#transcriber ul {
  position: relative;
}
div.transcribing div#transcriber ul form {
  position: relative;
  display: inline-block;
  vertical-align: top;
  height: 33px;
  margin: 0 5px 0 0;
}
div.transcribing div#transcriber ul form input[type="text"] {
  height: 13px;
  margin: 0 5px 0 0;
  padding: 9px 8px 9px;
  vertical-align: top;
  font: bold 11px "Helvetica",Arial;
  line-height: 13px!important;
  color: #999999;
  border: 1px solid black;
  text-transform: uppercase;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  -o-border-radius: 3px;
  -ms-border-radius: 3px;
  -khtml-border-radius: 3px;
  border-radius: 3px;
}
div.transcribing div#transcriber ul form input[type="text"].long {
  width: 208px;
}
div.transcribing div#transcriber ul form input[type="text"].medium {
  width: 118px;
}
div.transcribing div#transcriber ul form input[type="text"].short {
  width: 78px;
}
div.transcribing div#transcriber ul form input[type="text"]:focus {
  outline: none;
  color: #666666;
}
div.transcribing div#transcriber ul form.in input[type="text"] {
  padding-right: 36px;
}
div.transcribing div#transcriber ul form.in input[type="text"].long {
  width: 180px;
}
div.transcribing div#transcriber ul form.in input[type="text"].medium {
  width: 90px;
}
div.transcribing div#transcriber ul form.in input[type="text"].short {
  width: 50px;
}
div.transcribing div#transcriber ul form input[type="submit"] {
  margin: 0;
}
div.transcribing div#transcriber ul form input[type="submit"]:hover {
  cursor: pointer;
}
div.transcribing div#transcriber ul form.in input[type="submit"] {
  position: absolute;
  top: 3px;
  right: 8px;
  height: 27px;
  width: 27px;
  padding: 4px 4px 5px!important;
  border: none;
}
div.transcribing div#transcriber ul form.out input[type="submit"] {
  position: relative;
  height: 33px;
  padding: 6px 7px 7px;
  line-height: normal;
  margin: 0 5px 0 0;
}
div.transcribing div#transcriber ul form span.wrapper {
  position: relative;
  display: inline-block;
  margin: 0 5px 0 0;
}
div.transcribing div#transcriber ul form span.wrapper.short {
  width: 88px!important;
}
div.transcribing div#transcriber ul form span.wrapper.medium {
  width: 138px!important;
}
div.transcribing div#transcriber ul p {
  position: relative;
  display: inline-block;
  vertical-align: top;
  height: 15px;
  margin: 0;
  padding: 9px 14px;
  font: normal 13px "Helvetica",Arial;
  line-height: 15px;
  color: white;
  background: black;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  -o-border-radius: 3px;
  -ms-border-radius: 3px;
  -khtml-border-radius: 3px;
  border-radius: 3px;
}
div.transcribing div#transcriber ul p a {
  text-decoration: none;
  border-bottom: 1px solid white;
  color: white;
}
div.transcribing div#transcriber ul p a:hover {
  text-decoration: none;
  border-bottom: 1px dashed white;
}
div.transcribing div#transcriber ul p span.tail {
  position: absolute;
  left: -5px;
  top: 12px;
  width: 0px;
  height: 0px;
  border-top: 5px solid transparent;
  border-bottom: 5px solid transparent;
  border-right: 5px solid black;
}
div.transcribing div#transcriber ul.explanations > li, div.transcribing div#transcriber ul.titles > li {
  position: absolute;
  top: 0;
  left: 0;
  display: none;
}
div.transcribing div#transcriber ul.explanations > li:first-child, div.transcribing div#transcriber ul.titles > li:first-child {
  display: block;
}
div.transcribing div#transcriber ul.titles {
  top: -1px;
  left: -1px;
}
div.transcribing div#transcriber ul.titles li {
  padding: 5px 8px;
  font: bold 11px "Helvetica",Arial;
  color: white;
  border: none;
  border-right: 1px solid black;
  border-bottom: 1px solid black;
  background: #444444;
  -moz-border-radius-bottomright: 4px;
  -webkit-border-bottom-right-radius: 4px;
  -o-border-bottom-right-radius: 4px;
  -ms-border-bottom-right-radius: 4px;
  -khtml-border-bottom-right-radius: 4px;
  border-bottom-right-radius: 4px;
}
div.transcribing div#transcriber ul.steps {
  position: absolute;
  bottom: 45px;
  left: -88px;
  width: 136px;
  background: white;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  -o-border-radius: 2px;
  -ms-border-radius: 2px;
  -khtml-border-radius: 2px;
  border-radius: 2px;
  z-index: 1000;
  -moz-box-shadow: rgba(0, 0, 0, 0.2) 0 0 3px 2px;
  -webkit-box-shadow: rgba(0, 0, 0, 0.2) 0 0 3px 2px;
  -o-box-shadow: rgba(0, 0, 0, 0.2) 0 0 3px 2px;
  box-shadow: rgba(0, 0, 0, 0.2) 0 0 3px 2px;
}
div.transcribing div#transcriber ul.steps li {
  display: block;
  margin: 0;
  border-bottom: 1px solid #CCCCCC;
  -moz-border-radius: 2px;
  -webkit-border-radius: 2px;
  -o-border-radius: 2px;
  -ms-border-radius: 2px;
  -khtml-border-radius: 2px;
  border-radius: 2px;
}
div.transcribing div#transcriber ul.steps li:last-child {
  border: none;
}
div.transcribing div#transcriber ul.steps li a {
  display: block;
  padding: 6px 8px;
  font: normal 11px "Helvetica",Arial;
  color: #FF6600;
  text-decoration: none;
  
  opacity: 0.5;
}
div.transcribing div#transcriber ul.steps li span.circle {
  display: inline-block;
  vertical-align: middle;
  width: 4px;
  height: 4px;
  margin: -2px 5px 0 0;
